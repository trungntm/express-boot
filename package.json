{
  "name": "express-boot",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "yarn workspace @express-boot/app start",
    "dev": "yarn workspace @express-boot/app dev",
    "build": "yarn workspace @express-boot/starter-log build && yarn workspace @express-boot/starter-web build && yarn workspace @express-boot/app build",
    "build:packages": "node ./script.js",
    "test": "yarn workspace @express-boot/app test",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,json,md}\""
  },
  "dependencies": {
    "typedi": "^0.10.0",
    "reflect-metadata": "^0.2.1"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.11.19",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
